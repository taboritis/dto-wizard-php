{
    "name": "taboritis/dto-php",
    "description": "Data Transfer Objects for PHP",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "fakerphp/faker": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10",
        "symfony/var-dumper": "^6.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Taboritis\\DTO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taboritis\\DTO\\Examples\\": "examples/",
            "Tests\\Taboritis\\DTO\\": "tests/",
            "Tests\\Taboritis\\DTO\\Examples\\": "tests/Examples/"
        }
    },
    "authors": [
        {
            "name": "Piotr Skrobol",
            "email": "pio.skro@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
